{
"env": {
  "es6": true,
  "node": true,
  "jquery": true
  },
"parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
  },
"extends": [
  "eslint:recommended",
  "prettier"
  ],
"rules": {
  "func-style": ["error", "declaration"],
  "no-unused-vars": "off",
  "no-console": "off"
  }
}
